<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Library Category -->
    <record id="library_category" model="ir.module.category">
        <field name="name">Library Access</field>
    </record>

    <!-- Admin Group -->
    <record id="library_admin_group" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id"
               ref = "library_category" />
    </record>

    <!-- Libriran Group -->
    <record id="library_libriran_group" model="res.groups">
        <field name="name">Libriran</field>
        <field name="category_id"
               ref = "library_category" />
    </record>

</odoo>
